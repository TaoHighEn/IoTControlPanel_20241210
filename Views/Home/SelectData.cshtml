@using IoTControlPanel.Models;
@model IEnumerable<test>;
<table class="table-bordered">
    <tr>
        <th>GUID</th>
        <th>溫度</th>
        <th>濕度</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.GUID</td>
            <td>@item.TEMP</td>
            <td>@item.HUMI</td>
        </tr>
    }
</table>
